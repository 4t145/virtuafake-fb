{
    "server": {
        "host": "0.0.0.0:443",
        "tls": {
            "host": "0.0.0.0:443",
            "enable": false,
            "key": "/var/www/.cert/virtuafake.key",
            "cert": "/var/www/.cert/virtuafake.cert"
        }
    },

    "routes": {
        "/": "/var/www/virtuafake/index.html"
    },

    "static": {
        "directory": "/var/www/virtuafake/assets",
        "served_from": "/var/www/virtuafake/assets"
    },

    "config": {
        "enable_hot_reload": true,
        "fast_mem_cache": true,
        "enable_cache_control": true,
        "enable_directory_listing": false,
        "minify_html": false,
        "follow_symlinks": false,
        "enable_logging": false
    },

    "insert_headers": {
        "x-greetings": "七色的羁绊，吧恰图赖"
    }
}